<?xml version="1.0" encoding="utf-8"?>
<com.huodong.im.chatdemo.view.UserDefineScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/margin_gap"
    android:scrollbars="none"
    android:fillViewport="true"
    >
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
       	android:background="@drawable/corner_white_bg"
       	android:orientation="vertical"
        >
        <RelativeLayout 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <TextView 
        	android:id="@+id/title"
        	android:layout_width="wrap_content"
       	 	android:layout_height="wrap_content"
        	android:text="@string/date_title"
        	android:padding="@dimen/margin_gap_bigger"
        	android:textSize="@dimen/text_size_hint"
        	/>
            <EditText 
                android:id="@+id/date_title"
                android:layout_toRightOf="@id/title"
                android:layout_alignBottom="@id/title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/date_title_hint"
                android:singleLine="true"
                android:background="@null"
                android:gravity="center_vertical"
                android:padding="@dimen/margin_gap_bigger"
                />
            
        </RelativeLayout>
        <View 
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/grey"
            android:layout_marginLeft="@dimen/margin_gap_bigger"
            android:layout_marginRight="@dimen/margin_gap_bigger"/>
        <RelativeLayout 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <TextView 
        	android:id="@+id/partner_num"
        	android:layout_width="wrap_content"
       	 	android:layout_height="wrap_content"
        	android:text="@string/date_partner_num"
        	android:padding="@dimen/margin_gap_bigger"
        	android:textSize="@dimen/text_size_hint"
        	/>
            <EditText 
                android:id="@+id/date_partner_num"
                android:layout_toRightOf="@id/partner_num"
                android:layout_alignBottom="@id/partner_num"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/date_partner_num_hint"
                android:singleLine="true"
                android:background="@null"
                android:gravity="center_vertical"
                android:padding="@dimen/margin_gap_bigger"
                android:maxLength="1"
                android:numeric="integer"
                android:digits="123456789"
                />
        </RelativeLayout>
        <View 
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/grey"
            android:layout_marginLeft="@dimen/margin_gap_bigger"
            android:layout_marginRight="@dimen/margin_gap_bigger"/>
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <TextView 
                android:id="@+id/date_partner_type_nogender"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/date_partner_type_nogender"
                android:padding="@dimen/margin_gap_bigger"
                />
            <View 
            	android:layout_width="0.5dp"
            	android:layout_height="match_parent"
            	android:background="@color/grey"
            	android:layout_marginTop="@dimen/margin_gap_bigger"
            	android:layout_marginBottom="@dimen/margin_gap_bigger"
            	android:textSize="@dimen/text_size_hint"/>
            <TextView 
                android:id="@+id/date_partner_type_women"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/date_partner_type_women"
                android:padding="@dimen/margin_gap_bigger"
                />
            <View 
            	android:layout_width="0.5dp"
            	android:layout_height="match_parent"
            	android:background="@color/grey"
            	android:layout_marginTop="@dimen/margin_gap_bigger"
            	android:layout_marginBottom="@dimen/margin_gap_bigger"/>
            <TextView 
                android:id="@+id/date_partner_type_man"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/date_partner_type_man"
                android:padding="@dimen/margin_gap_bigger"
                />
        </LinearLayout>
        <View 
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/grey"
            android:layout_marginLeft="@dimen/margin_gap_bigger"
            android:layout_marginRight="@dimen/margin_gap_bigger"/>
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <TextView 
                android:id="@+id/date_fees_type_i"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3.2"
                android:text="@string/date_fees_type_i"
                android:padding="@dimen/margin_gap_bigger"
                />
            <View 
            	android:layout_width="0.5dp"
            	android:layout_height="match_parent"
            	android:background="@color/grey"
            	android:layout_marginTop="@dimen/margin_gap_bigger"
            	android:layout_marginBottom="@dimen/margin_gap_bigger"/>
            
            <TextView 
                android:id="@+id/date_fees_type_man_a"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"
                android:text="@string/date_fees_type_man_a"
                android:padding="@dimen/margin_gap_bigger"/>
            <View 
            	android:layout_width="0.5dp"
            	android:layout_height="match_parent"
            	android:background="@color/grey"
            	android:layout_marginTop="@dimen/margin_gap_bigger"
            	android:layout_marginBottom="@dimen/margin_gap_bigger"/>
            <TextView 
                android:id="@+id/date_fees_type_aa"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2.2"
                android:text="@string/date_fees_type_aa"
                android:padding="@dimen/margin_gap_bigger"/>
           <View 
            	android:layout_width="0.5dp"
            	android:layout_height="match_parent"
            	android:background="@color/grey"
            	android:layout_marginTop="@dimen/margin_gap_bigger"
            	android:layout_marginBottom="@dimen/margin_gap_bigger"/>
            <TextView 
                android:id="@+id/date_fees_type_u"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:text="@string/date_fees_type_u"
                android:padding="@dimen/margin_gap_bigger"/>
        </LinearLayout>
        <View 
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/grey"
            android:layout_marginLeft="@dimen/margin_gap_bigger"
            android:layout_marginRight="@dimen/margin_gap_bigger"/>
        <RelativeLayout 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <TextView 
        	android:id="@+id/time"
        	android:layout_width="wrap_content"
       	 	android:layout_height="wrap_content"
        	android:text="@string/date_time_tip"
        	android:padding="@dimen/margin_gap_bigger"
        	android:textSize="@dimen/text_size_hint"
        	/>
            <TextView 
                android:id="@+id/date_time"
                android:layout_toRightOf="@id/time"
                android:layout_alignBottom="@id/time"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/date_time_tip_hint"
                android:singleLine="true"
                android:background="@null"
                android:gravity="center_vertical"
                android:padding="@dimen/margin_gap_bigger"
                android:textSize="@dimen/text_size_hint"
                />
        </RelativeLayout>
        <View 
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/grey"
            android:layout_marginLeft="@dimen/margin_gap_bigger"
            android:layout_marginRight="@dimen/margin_gap_bigger"/>
        <RelativeLayout 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <TextView 
        	android:id="@+id/address"
        	android:layout_width="wrap_content"
       	 	android:layout_height="wrap_content"
        	android:text="@string/date_address_tip"
        	android:padding="@dimen/margin_gap_bigger"
        	android:textSize="@dimen/text_size_hint"
        	/>
            <TextView 
                android:id="@+id/date_address"
                android:layout_toRightOf="@id/address"
                android:layout_alignBottom="@id/address"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/date_address_tip_hint"
                android:singleLine="true"
                android:background="@null"
                android:gravity="center_vertical"
                android:textSize="@dimen/text_size_hint"
                android:padding="@dimen/margin_gap_bigger"
                />
        </RelativeLayout>
        <View 
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/grey"
            android:layout_marginLeft="@dimen/margin_gap_bigger"
            android:layout_marginRight="@dimen/margin_gap_bigger"/>
        <RelativeLayout 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <TextView 
        	android:id="@+id/notes"
        	android:layout_width="wrap_content"
       	 	android:layout_height="wrap_content"
        	android:text="@string/date_note_tip"
        	android:padding="@dimen/margin_gap_bigger"
        	android:textSize="@dimen/text_size_hint"
        	/>
            <EditText 
                android:id="@+id/date_notes"
                android:layout_toRightOf="@id/notes"
                android:layout_alignBottom="@id/notes"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/date_note_tip_hint"
                android:singleLine="true"
                android:background="@null"
                android:gravity="center_vertical"
                android:padding="@dimen/margin_gap_bigger"
                />
        </RelativeLayout>
        <Button 
            android:id="@+id/btn_release_date"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/date_release_tip"
            android:background="@drawable/btn_blue"
            android:padding="@dimen/margin_gap"
            android:layout_margin="@dimen/margin_gap"
            android:textColor="@color/white_text"
            android:textSize="@dimen/text_size_hint"
            android:clickable="true"
            />
        <View 
            android:layout_width="match_parent"
            android:layout_height="100dp"/>
        
        <!-- <Button 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/date_release_tip"
            android:background="@drawable/btn_blue"
            android:padding="@dimen/margin_gap"
            android:layout_margin="@dimen/margin_gap"
            android:textColor="@color/white_text"
            android:textSize="@dimen/text_size_hint"
            android:clickable="true"
            />
        <Button 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/date_release_tip"
            android:background="@drawable/btn_blue"
            android:padding="@dimen/margin_gap"
            android:layout_margin="@dimen/margin_gap"
            android:textColor="@color/white_text"
            android:textSize="@dimen/text_size_hint"
            android:clickable="true"
            />
        <Button 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/date_release_tip"
            android:background="@drawable/btn_blue"
            android:padding="@dimen/margin_gap"
            android:layout_margin="@dimen/margin_gap"
            android:textColor="@color/white_text"
            android:textSize="@dimen/text_size_hint"
            android:clickable="true"
            /> -->
        
        
    </LinearLayout>
    
    
</com.huodong.im.chatdemo.view.UserDefineScrollView>
