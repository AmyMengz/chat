            	android:layout_marginBottom="@dimen/margin_gap"
            	/>
         	<TextView 
            android:id="@+id/fees_limit_men_a"
        	android:layout_width="0dp"
        	android:layout_weight="1"
        	android:gravity="center"
        	android:layout_height="wrap_content"
        	android:text="@string/selection_fees_men_a"
        	android:paddingTop="@dimen/margin_gap_bigger"
        	android:paddingBottom="@dimen/margin_gap_bigger"
        	android:textSize="@dimen/text_size_little"
        	/>
         </LinearLayout>
         <View 
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/grey"
            android:layout_marginLeft="@dimen/margin_gap_bigger"
            android:layout_marginRight="@dimen/margin_gap_bigger"/>
         <LinearLayout 
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:background="@color/white"
             android:orientation="horizontal"
             >
         	<TextView 
            android:id="@+id/fees_limit_aa"
        	android:layout_width="0dp"
        	android:layout_weight="1"
        	android:gravity="center"
        	android:layout_height="wrap_content"
        	android:text="@string/selection_fees_aa"
        	android:paddingTop="@dimen/margin_gap"
        	android:paddingBottom="@dimen/margin_gap"
        	android:textSize="@dimen/text_size_little"
        	/>
         	<View 
            	android:layout_width="0.5dp"
            	android:layout_height="match_parent"
            	android:background="@color/grey"
            	android:layout_marginTop="@dimen/margin_gap"
            	android:layout_marginBottom="@dimen/margin_gap"
            	/>
         	<TextView 
            android:id="@+id/fees_limit_partner"
        	android:layout_width="0dp"
        	android:layout_weight="1"
        	android:gravity="center"
        	android:layout_height="wrap_content"
        	android:text="@string/selection_fees_partner"
        	android:paddingTop="@dimen/margin_gap_bigger"
        	android:paddingBottom="@dimen/margin_gap_bigger"
        	android:textSize="@dimen/text_size_little"
        	/>
         </LinearLayout>
         
         <TextView 
            android:id="@+id/time_limit"
        	android:layout_width="match_parent"
        	android:layout_height="wrap_content"
        	android:text="@string/selection_date_time"
        	android:paddingTop="@dimen/margin_gap_bigger"
        	android:paddingBottom="@dimen/margin_gap_bigger"
