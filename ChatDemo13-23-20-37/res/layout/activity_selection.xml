        	android:paddingTop="@dimen/margin_gap_bigger"
        	android:paddingBottom="@dimen/margin_gap_bigger"
        	android:textSize="@dimen/text_size_little"
        	/>
             <View 
            	android:layout_width="0.5dp"
            	android:layout_height="match_parent"
            	android:background="@color/grey"
            	android:layout_marginTop="@dimen/margin_gap"
            	android:layout_marginBottom="@dimen/margin_gap"
            	/>
         	<TextView 
