        	android:paddingTop="@dimen/margin_gap_bigger"
        	android:paddingBottom="@dimen/margin_gap_bigger"
        	android:textSize="@dimen/text_size_little"
        />
         <LinearLayout 
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:background="@color/white"
             android:orientation="horizontal"
             >
             <TextView 
            android:id="@+id/active_limit_no"
        	android:layout_width="0dp"
        	android:layout_weight="1"
        	android:gravity="center"
        	android:layout_height="wrap_content"
        	android:text="@string/selection_user_active_no"
        	android:paddingTop="@dimen/margin_gap_bigger"
        	android:paddingBottom="@dimen/margin_gap_bigger"
        	android:textSize="@dimen/text_size_little"
        	/>
             <View 
            	android:layout_width="0.5dp"
            	android:layout_height="match_parent"
            	android:background="@color/grey"
            	android:layout_marginTop="@dimen/margin_gap"
            	android:layout_marginBottom="@dimen/margin_gap"
            	/>
         	<TextView 
            android:id="@+id/active_limit_one"
        	android:layout_width="0dp"
        	android:layout_weight="1"
        	android:gravity="center"
        	android:layout_height="wrap_content"
        	android:text="@string/selection_user_active_one"
        	android:paddingTop="@dimen/margin_gap"
        	android:paddingBottom="@dimen/margin_gap"
        	android:textSize="@dimen/text_size_little"
        	/>
         	
