<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <include
        android:id="@+id/common_actionbar"
        layout="@layout/include_action_bar" />
    <com.huodong.im.chatdemo.view.UserDefineScrollView 
   	android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none"
    >
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
    <LinearLayout 
        android:id="@+id/date_select"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="@dimen/margin_gap"
        >
         <TextView 
            android:id="@+id/gender_limit"
        	android:layout_width="match_parent"
        	android:layout_height="wrap_content"
        	android:text="@string/selection_gender"
        	android:paddingTop="@dimen/margin_gap_bigger"
        	android:paddingBottom="@dimen/margin_gap_bigger"
        	android:textSize="@dimen/text_size_little"
        />
         <LinearLayout 
             
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:background="@color/white"
             android:orientation="horizontal"
             >
             <TextView 
            android:id="@+id/gender_limit_no"
        	android:layout_width="0dp"
        	android:layout_weight="1"
        	android:gravity="center"
        	android:layout_height="wrap_content"
        	android:text="@string/selection_gender_no"
        	android:paddingTop="@dimen/margin_gap_bigger"
        	android:paddingBottom="@dimen/margin_gap_bigger"
        	android:textSize="@dimen/text_size_little"
        	/>
             <View 
            	android:layout_width="0.5dp"
            	android:layout_height="match_parent"
            	android:background="@color/grey"
            	android:layout_marginTop="@dimen/margin_gap"
            	android:layout_marginBottom="@dimen/margin_gap"
            	/>
         	<TextView 
            android:id="@+id/gender_limit_women"
        	android:layout_width="0dp"
        	android:layout_weight="1"
        	android:gravity="center"
        	android:layout_height="wrap_content"
        	android:text="@string/selection_gender_women"
        	android:paddingTop="@dimen/margin_gap"
        	android:paddingBottom="@dimen/margin_gap"
        	android:textSize="@dimen/text_size_little"
