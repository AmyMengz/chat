            	android:layout_marginBottom="@dimen/margin_gap"
            	/>
         	<TextView 
            android:id="@+id/gender_limit_men"
        	android:layout_width="0dp"
        	android:layout_weight="1"
        	android:gravity="center"
        	android:layout_height="wrap_content"
        	android:text="@string/selection_gender_men"
        	android:paddingTop="@dimen/margin_gap_bigger"
        	android:paddingBottom="@dimen/margin_gap_bigger"
        	android:textSize="@dimen/text_size_little"
        	/>
         </LinearLayout>
         <TextView 
            android:id="@+id/fees_limit"
        	android:layout_width="match_parent"
        	android:layout_height="wrap_content"
        	android:text="@string/selection_fees"
        	android:paddingTop="@dimen/margin_gap_bigger"
        	android:paddingBottom="@dimen/margin_gap_bigger"
        	android:textSize="@dimen/text_size_little"
        />
         <LinearLayout 
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:background="@color/white"
             android:orientation="horizontal"
             >
             <TextView 
